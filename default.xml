<?xml version="1.0" encoding="UTF-8" ?>

<manifest>
    <!-- 
    remote: 远程代码库配置
    name: 远程仓库名，在使用 git clone 时默认为 origin
    fetch: 远程仓库地址，用户和 project 节点中的 name 一起确定项目地址
    -->
    <!-- remote server -->
    <remote
        name="github"
        fetch="https://github.com" />
	<remote  name="ccwav"
       fetch="https://github.com/ccwav"
       revision="main"
       review="" />
    <!--
    default: 默认配置
    remote: 默认使用的远程仓库
    revision: 默认使用的分支
    sync-j: 同步代码时的并发数
    -->
    <default
        remote="github"
        revision="main"
        sync-j="1" />
	<project name="QLScript2" path="." groups="jd" remote="ccwav">
		<copyfile src="jdCookie.js" dest="jdCookie.js"></copyfile>
		<copyfile src="JS_USER_AGENTS.js" dest="JS_USER_AGENTS.js"></copyfile>
		<copyfile src="USER_AGENTS.js" dest="USER_AGENTS.js"></copyfile>
		<copyfile src="ql.js" dest="UpdatDest\ql.js"></copyfile>
		<copyfile src="ql.js" dest="UpdatDest\ql.js"></copyfile>
		<copyfile src="sendNotify.js" dest="UpdatDest\sendNotify.js"></copyfile>
	</project>
	<project name="QLScript2/README.md" path="test.md" groups="jd" remote="ccwav"/>
</manifest>